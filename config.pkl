amends "https://raw.githubusercontent.com/kaihendry/actions/refs/heads/main/terraform/TerraformConfig.pkl"

terraform {
  resource {
    aws_ssm_parameter {
      ["my_app_api_key"] {
        name = "/my-app/api-key"
        description = "API key for a third-party service"
        type = "SecureString"
        value = read("env:APIKEY")
        tags {
          ["project"] = "my-app"
        }
      }
    }
  }
}
